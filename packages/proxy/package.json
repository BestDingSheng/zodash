{
  "name": "@ghost/proxy",
  "version": "0.0.1",
  "description": "a simple proxy",
  "main": "lib/index.js",
  "module": "src/index.ts",
  "repository": "https://github.com/zcorky/zodash",
  "homepage": "https://github.com/zcorky/zodash",
  "bugs": {
    "url": "https://github.com/zcorky/zodash/issues"
  },
  "keywords": [
    "zcorky",
    "zodash",
    "proxy",
    "node",
    "koa",
    "typescript"
  ],
  "author": "Zero <uniquecolesmith@gmail.com> (https://moeover.com)",
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && tsc",
    "watch": "tsc -w",
    "clean": "rimraf -rf lib",
    "test": "jest -w 1",
    "coverage": "jest -w 1 && codecov",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/egg": "^1.5.0",
    "@types/jest": "^24.0.11",
    "@types/koa": "^2.0.49",
    "@types/node": "^10.9.4",
    "@types/node-fetch": "^2.5.0",
    "@types/type-is": "^1.6.3",
    "codecov": "^3.2.0",
    "cross-env": "^5.2.0",
    "husky": "^1.2.1",
    "jest": "^24.5.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^24.0.0",
    "ts-node": "^8.5.2",
    "tslint": "^5.10.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-jsdoc-rules": "^0.1.2",
    "typescript": "^3.2.2"
  },
  "files": [
    "lib/"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "npm test"
    }
  },
  "jest": {
    "roots": [
      "test"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "test/.*\\.spec\\.(ts|tsx)$",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "coverageDirectory": "./coverage/",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 90,
        "lines": 90,
        "statements": -10
      }
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@koex/core": "^0.0.20",
    "@types/debug": "^4.1.5",
    "@zcorky/is": "^1.0.6",
    "@zcorky/lru": "^1.0.3",
    "@zcorky/query-string": "^1.0.2",
    "@zodash/crypto": "^0.0.7",
    "@zodash/format": "^1.0.1-alpha.0",
    "@zodash/logger": "^0.0.5",
    "@zodash/omit": "^1.0.16",
    "@zodash/onion": "^0.0.9",
    "debug": "^4.1.1",
    "node-fetch": "^2.6.0",
    "type-is": "^1.6.18"
  }
}
